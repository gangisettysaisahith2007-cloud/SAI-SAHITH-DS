#include <stdio.h>
#include <ctype.h>

int isValidString(char *s) {
    for (int i = 0; s[i] != '\0'; i++) {
        if (!isalnum(s[i])) // not alphabet or digit
            return 0;
    }
    return 1;
}

int main() {
    char str[100];
    printf("Enter string: ");
    scanf("%s", str);

    if (isValidString(str))
        printf("Valid String\n");
    else
        printf("Invalid String\n");

    return 0;
}
