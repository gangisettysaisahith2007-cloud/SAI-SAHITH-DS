#include <stdio.h>
int main() {
    int a[5] = {10, 20, 30, 40, 50};
    int i, key, found = 0;

    printf("Enter number to search: ");
    scanf("%d",&key);

    for(i=0;i<5;i++) {
        if(a[i]==key) {
            printf("Found at position %d\n", i+1);
            found = 1;
            break;
        }
    }
    if(!found) printf("Not Found\n");
    return 0;
}
