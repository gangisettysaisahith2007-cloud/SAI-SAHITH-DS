#include <stdio.h>
int main() {
    int a[7] = {5, 10, 15, 20, 25, 30, 35};
    int key, low=0, high=6, mid, found=0;

    printf("Enter number to search: ");
    scanf("%d",&key);

    while(low <= high) {
        mid = (low + high) / 2;
        if(a[mid] == key) {
            printf("Found at position %d\n", mid+1);
            found = 1;
            break;
        }
        else if(a[mid] < key)
            low = mid + 1;
        else
            high = mid - 1;
    }

    if(!found) printf("Not Found\n");
    return 0;
}
