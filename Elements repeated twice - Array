#include <stdio.h>

int main() {
    int arr[50], n, i, j, count;

    printf("Enter size of array: ");
    scanf("%d", &n);

    printf("Enter elements: ");
    for (i = 0; i < n; i++) scanf("%d", &arr[i]);

    printf("Elements repeated twice: ");
    for (i = 0; i < n; i++) {
        count = 0;
        for (j = 0; j < n; j++) {
            if (arr[i] == arr[j])
                count++;
        }
        if (count == 2) {
            printf("%d ", arr[i]);
            // avoid printing same number again
            for (j = 0; j < n; j++) {
                if (arr[j] == arr[i])
                    arr[j] = -1; // mark as visited
            }
        }
    }

    return 0;
}
