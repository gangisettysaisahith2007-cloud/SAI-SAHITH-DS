#include <stdio.h>

int main() {
    int arr[50], n, i;

    printf("Enter size of array: ");
    scanf("%d", &n);

    printf("Enter elements: ");
    for (i = 0; i < n; i++)
        scanf("%d", &arr[i]);

    if (n >= 5)
        printf("5th iterated element = %d\n", arr[4]); // index 4 = 5th element
    else
        printf("Array has less than 5 elements!\n");

    return 0;
}
